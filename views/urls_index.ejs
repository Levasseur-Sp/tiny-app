<!DOCTYPE html>
<html lang="en">

<head>
  <title>TinyApp</title>
</head>

<body>
  <% include partials/_header %>
  <% for (let url in urls){ %>
  <form method="POST" action="/urls/<%= url %>/delete">
    <table>
      <tr>
        <td>
          <%= urls[url] %>
        </td>
        <td>
          <%= url %>
        </td>
        <td>
          <input id="<%= url %>" name="<%= url %>" type="submit" value="delete" />
        </td>
      </tr>
    </table>
  </form>
  <% }; %>
  <a href="#">Shorten your URL!</a>
</body>

</html>