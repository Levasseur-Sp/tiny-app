<!DOCTYPE html>
<html lang="en">

<% include partials/_head%>

<body>
  <% include partials/_header %>
  <% for (let url in urls){ %>
  <form method="POST" action="/urls/<%= url %>/delete">
    <table>
      <tr>
        <td>
          <%= url %>
        </td>
        <td>
          <%= urls[url] %>
        </td>
        <td>
          <input id="<%= url %>" name="<%= url %>" type="submit" value="delete" />
        </td>
      </tr>
    </table>
  </form>
  <% }; %>
  <% include partials/_footer %>
</body>

</html>